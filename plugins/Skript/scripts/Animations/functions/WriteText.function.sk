    remove (length of {_text}/3) from x coordinate of {_location::start}
  else if {_direction} is "z":
    remove (length of {_text}/3) from z coordinate of {_location::start}

  set {_location::current} to {_location::start}

  set {_location::current}'s yaw to 90
  set {_location::current}'s pitch to 0

  # Спавним сами буквы и анимируем их
  set {_words::*} to {_text} split at ""
  loop {_words::*}:
    # Отчистка ненужных переменных
    delete {_word::icon}

    # Подбираем иконку под букву
    set {_word::icon} to {_word::%loop-value%}

    # Спавним армор стенд (если иконка есть)
    {_word::icon} is set
    spawn armor stand at {_location::current} with nbt "{Invisible:1}"

    set {_entity} to last spawned armor stand 
    # Надеваем голову на ентити
    make {_entity} wear {_word::icon}

    loop all players:
      if loop-value-2 isn't {_player}:
        packetDestroyEntity(loop-value-2, {_entity})

    # Анимируем...
    set {_coordinate} to {_location}'s y coordinate
    WriteText_pushWordUp({_entity}, {_coordinate})

    if {_direction} is "x":
      add 1 to x coordinate of {_location::current}
    else if {_direction} is "z":
      add 1 to z coordinate of {_location::current}
    add {_entity} to {_entities::*}

    wait 5 ticks

  wait {_wait}
  loop {_entities::*}:
    WriteText_KillWord(loop-value, {_player})

    wait 2 ticks

function WriteText_KillWord(entity: entity, player: player):
  {_entity}.setGravity(true)

  while block below {_entity} is air:
    wait 1 tick

  set {_location} to {_entity}'s location
  add 1.5 to {_location}'s y coordinate
  play explosion with offset of 0, 0, 0 on {_location} to {_player}
  kill {_entity}

# word-related
function WriteText_pushWordUp(entity: entity, yCoordinate: number):
  push {_entity} upwards at speed 0.5

  while {_entity}'s y coordinate > {_yCoordinate} - 0.5:
    wait 1 tick
  {_entity}.setGravity(false)