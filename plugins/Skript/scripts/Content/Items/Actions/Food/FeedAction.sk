function AC_FeedAction(player: player, foodCount: string):
  set {_foodCount} to "%{_foodCount}%" parsed as number
  
  add {_foodCount} to {emotions::%{_player}%::hunger::percent}
  setCharacterHungerLevel({_player}, getCurrentCharacter({_player}), {emotions::%{_player}%::hunger::percent})

  set {_item} to {_player}'s held item
  remove 1 of {_item} from {_player}'s inventory